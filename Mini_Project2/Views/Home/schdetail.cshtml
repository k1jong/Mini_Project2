
@{
    ViewData["Title"] = "Result";
}

<style>
	.game-cell {
		width: 200px; /* 적절한 너비 설정 */
		white-space: nowrap; /* 텍스트 줄 바꿈 방지 */
		overflow: hidden; /* 너무 긴 텍스트는 숨기기 */
		text-overflow: ellipsis; /* 너무 긴 텍스트는 ...으로 표시 */
	}



	.image-frames img {
		max-width: 80px; /* 원하는 크기로 조절하세요 */
		max-height: 80px; /* 원하는 크기로 조절하세요 */
	}
	/* 표 전체 스타일링 */
	.lst_schedule {
		width: 100%;
		border-collapse: collapse;
		margin-bottom: 20px;
	}
		/* 캡션 스타일링 */
		.lst_schedule caption {
			font-weight: bold;
			margin-bottom: 20px;
		}

		/* 헤더 셀 스타일링 */
		.lst_schedule th {
			background-color: #f2f2f2;
			border: 1px solid #ddd;
			text-align: center;
			padding: 8px;
		}

		/* 데이터 셀 스타일링 */
		.lst_schedule td {
			border: 1px solid #ddd;
			text-align: center;
			padding: 8px;
		}

	h4 {
		border: 1px solid #ddd; /* 테두리 스타일 설정 */
		background-color: navy; /* 배경색 설정 */
		color: #fff; /* 텍스트 색상 설정 (진한 회색) */
		padding: 13px;
	}

	h3 {
		border: 1px solid #ddd; /* 테두리 스타일 설정 */
		background-color: #808080; /* 배경색 설정 */
		color: #fff; /* 텍스트 색상 설정 (진한 회색) */
		padding: 15px;
	}


	.date {
		color: navy;
	}
	/* 경기 셀 스타일링 */
	.game-cell {
		white-space: nowrap;
		overflow: hidden;
		text-overflow: ellipsis;
	}

	ul {
		list-style: none;
		padding: 0;
		margin: 0;
		display: flex; /* 요소들을 가로로 나란히 배치  */
	}

	li {
		flex: 1; /* 가로 공간을 동일하게 분할하여 배치 */
		text-align: center; /* 텍스트 가운데 정렬 */
		list-style: none
	}

	.wrp_lst table {
		width: 200%;
		max-width: 1300px;
	}

		.wrp_lst table tr {
			height: 30px; /* 원하는 높이로 조정 */
		}

</style>

