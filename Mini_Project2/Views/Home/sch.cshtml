<canvas id="myChart"></canvas>
<script src="https://cdn.jsdelivr.net/npm/chart.js@3.9.0/dist/chart.min.js"></script>
<script>
	const DATA_COUNT = 6;
	const NUMBER_CFG = { count: DATA_COUNT, min: -100, max: 100 };

	const labels = ['도드람 2017-2018 V-리그', '도드람 2018-2019 V-리그', '도드람 2019-2020 V-리그', '도드람 2020-2021 V-리그', '도드람 2021-2022 V-리그', '도드람 2022-2023 V-리그'];
	const data = {
		labels: labels,
		datasets: [
			{
				label: '흥국생명 핑크스파이더스',
				data: [6, 1, 3, 2, 6, 1],
				borderColor: 'red',
				backgroundColor: 'rgba(255, 0, 0, 0.5)',
				yAxisID: 'y',
			},
			{
				label: '현대건설 힐스테이트',
				data: [3, 5, 1, 6, 1, 2],
				borderColor: 'blue',
				backgroundColor: 'rgba(0, 0, 255, 0.5)',
				yAxisID: 'y',
			},
			{
				label: '한국도로공사 하이패스 배구단',
				data: [1, 2, 6, 4, 2, 3],
				borderColor: 'green',
				backgroundColor: 'rgba(0, 255, 0, 0.5)',
				yAxisID: 'y',
			},
			{
				label: 'KGC인삼공사 프로배구단',
				data: [5, 6, 4, 5, 4, 4],
				borderColor: 'orange',
				backgroundColor: 'rgba(255, 165, 0, 0.5)',
				yAxisID: 'y',
			},
			{
				label: 'GS 칼텍스 KIXX',
				data: [4, 3, 2, 1, 3, 5],
				borderColor: 'purple',
				backgroundColor: 'rgba(128, 0, 128, 0.5)',
				yAxisID: 'y',
			},
			{
				label: 'IBK 기업은행 알토스',
				data: [2, 4, 5, 3, 5, 6],
				borderColor: 'cyan',
				backgroundColor: 'rgba(0, 255, 255, 0.5)',
				yAxisID: 'y',
			},
			{
				label: '페퍼저축은행 AI 페퍼스',
				data: [null, null, null, 7, 7, 7],
				borderColor: 'pink',
				backgroundColor: 'rgba(255, 182, 193, 0.5)',
				yAxisID: 'y',
			}
		]
	};

	const ctx = document.getElementById('myChart').getContext('2d');
	const myChart = new Chart(ctx, {
		type: 'line',
		data: data,
		options: {
			scales: {
				y: {
					beginAtZero: true,
					reverse: true,
				}
			}
		}
	});
</script>
